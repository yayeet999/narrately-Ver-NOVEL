{
  "version": 2,
  "name": "narrately-novel-real",
  "functions": {
    "api/o1_structure.js": {
      "memory": 2048,
      "maxDuration": 480
    }
  },
  "routes": [
    {
      "src": "/api/o1_structure",
      "dest": "/api/o1_structure.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key"
  }
}
