{
  "version": 2,
  "buildCommand": "next build",
  "outputDirectory": ".next/out",
  "framework": "nextjs",
  "routes": [
    {
      "src": "^/api/(.*)$",
      "dest": "/api/$1",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "continue": true
    }
  ]
} 